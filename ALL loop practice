      i++;
    }
  }
}
using System;

class Program {
  public static void Main (string[] args) {
    int n = 5;
    for (int i = 1; i <= n; i++)
    {
      for (int j = 1; j<= i; j++)
      {
        Console.Write("*");
      }
      Console.WriteLine();
    }
  }
}
using System;

class Program 
{
  public static void Main (string[] args) 
  {
    int n = 4;
    for (int i = 1; i <= n; i++)
    {
      for (int j = 1; j <= 8; j++)
      {
        Console.Write("*");
      }
      Console.WriteLine();
    }  
  }
}
using System;

class Program
{
    public static void Main(string[] args)
    {
        int n = 3;
        for (int i = 1; i <= n; i++)
        {
            for (int j = 1; j <= 10; j++)
            {
                Console.WriteLine((i * j) + "\t");
            }
            Console.WriteLine();
        }
        

}
}  
